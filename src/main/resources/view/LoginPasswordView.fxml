<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="311.0" prefWidth="363.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep12.jdbc.sql_injection.controller.LoginPasswordViewController">
   <children>
      <Label alignment="CENTER" layoutX="73.0" layoutY="47.0" prefHeight="26.0" prefWidth="221.0" text="LOGIN">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="72.0" layoutY="93.0" prefHeight="21.0" prefWidth="221.0" text="password :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="txtPassword" alignment="CENTER" layoutX="72.0" layoutY="137.0" prefHeight="28.0" prefWidth="221.0" promptText="Enter your password here">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <HBox alignment="CENTER" layoutX="72.0" layoutY="184.0" prefHeight="28.0" prefWidth="221.0" spacing="20.0">
         <children>
            <Button fx:id="btnLogin" defaultButton="true" mnemonicParsing="false" onAction="#btnLoginOnAction" text="LOGIN">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnExit" mnemonicParsing="false" onAction="#btnExitOnAction" text="EXIT">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <Button fx:id="btnBack" layoutX="155.0" layoutY="238.0" mnemonicParsing="false" onAction="#btnBackOnAction" style="-fx-background-color: RED;" text="BACK" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
