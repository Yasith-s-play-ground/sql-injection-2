<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="277.0" prefWidth="339.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep12.jdbc.sql_injection.controller.LoginUserNameViewController">
   <children>
      <Label alignment="CENTER" layoutX="53.0" layoutY="21.0" prefHeight="26.0" prefWidth="258.0" text="LOGIN">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <TextField fx:id="txtUserName" alignment="CENTER" layoutX="43.0" layoutY="126.0" prefHeight="28.0" prefWidth="258.0" promptText="Enter your username here">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label alignment="CENTER" layoutX="59.0" layoutY="86.0" prefHeight="21.0" prefWidth="247.0" text="username :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="65.0" layoutY="57.0" prefHeight="21.0" prefWidth="214.0" text="Welcome">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" layoutX="45.0" layoutY="174.0" prefHeight="28.0" prefWidth="258.0" spacing="20.0">
         <children>
            <Button fx:id="btnContinue" defaultButton="true" mnemonicParsing="false" onAction="#btnContinueOnAction" text="CONTINUE">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnExit" mnemonicParsing="false" onAction="#btnExitOnAction" text="EXIT">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <Button fx:id="btnRegister" layoutX="100.0" layoutY="219.0" mnemonicParsing="false" onAction="#btnRegisterOnAction" text="Register New User">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
